#!/bin/bash

# Check if Python is installed
if command -v python3 &>/dev/null; then
    echo "Python found, running script..."
    python3 2-print.py
elif command -v python &>/dev/null; then
    echo "Python found, running script..."
    python 2-print.py
else
    echo "Python is not installed. Please install Python to run this script."
    exit 1
fi
